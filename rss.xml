&lt;p&gt;&#x6211;&#x4EEC;&#x53EF;&#x4EE5;&#x60F3;&#x50CF;&#x4E00;&#x4E2A;&#x65E0;&#x7A77;&#x9012;&#x5F52;&#x7684;&#x51FD;&#x6570;&#xFF1A;&lt;/p&gt;
&lt;pre class=&quot;sourceCode CommonLisp&quot;&gt;&lt;code class=&quot;sourceCode commonlisp&quot;&gt;(&lt;span class=&quot;kw&quot;&gt;defun&lt;/span&gt;&lt;span class=&quot;fu&quot;&gt; have-a-good-idea &lt;/span&gt;()
  (found-a-company)
  (fail)
